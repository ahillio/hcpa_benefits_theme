function hcpa_benefits_date_combo($variables) {
  return theme('form_element', $variables);
}

function hcpa_benefits_tuition_assistance_entityform_edit_form_alter(&$form, $form_id){
  $form['agreement'] = array(
    '#markup' => t('<p>By submitting this application, I agree to abide by <a class="colorbox-inline" href="?width=410&amp;height=660&amp;inline=true#block-bean-tuition-assistance-rules-and-r">the rules and requirements</a> governing the SEIU HCPA Tuition Assistance Program as approved by the SEIU Healthcare PA Training and Education Fund Board of Trustees.</p>'),
    '#weight' => 30,
  );
}
